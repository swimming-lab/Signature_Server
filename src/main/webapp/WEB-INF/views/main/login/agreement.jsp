                    confirmButtonText: '확인',
                    customClass: 'custom-small-title'
                });
                return;
            }
        });
        $('#allAgreement').on('click', (event) => {
            event.preventDefault();
            if($("input:checkbox[id='allAgreement']").is(":checked") == true){
                $('#serviceAgreement').iCheck('uncheck');
