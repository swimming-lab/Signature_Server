    <script data-for="ready">
        $('.i-checks').iCheck({
            checkboxClass: 'iradio_square-yellow',
        });

        // Add slimscroll to element
        $('.scroll_content').slimscroll({
            height: '125px'
        })
        $('.scroll_content2').slimscroll({
            height: '125px'
        })
        $('#cancel').on('click', (event) => {
